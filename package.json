{
  "name": "key-board-hook",
  "version": "1.0.0",
  "main": "dist/key-board-hook.umd.js",
  "module": "dist/key-board-hook.esm.js",
  "unpkg": "dist/key-board-hook.min.js",
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/key-board-hook.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/key-board-hook.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/key-board-hook.min.js"
  },
  "devDependencies": {
    "rollup": "^1.1.2",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-uglify": "^6.0.2"
  }
}
